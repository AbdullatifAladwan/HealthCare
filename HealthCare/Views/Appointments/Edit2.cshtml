
@{
    ViewData["Title"] = "Edit2";
    Layout = "~/Views/Shared/_LayoutDoctor.cshtml";
}


@model HealthCare.Models.Appointment



<h1>Edit</h1>

<h4>Appointment</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit2">
            
            <div class="form-group">
                <label asp-for="AcceptOrReject" class="control-label"></label>
                <input asp-for="AcceptOrReject" data-something="@Model.AcceptOrReject" class="form-control" />
                <span asp-validation-for="AcceptOrReject" class="text-danger"></span>
            </div>
           
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
